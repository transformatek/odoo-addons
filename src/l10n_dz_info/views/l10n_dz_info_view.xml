<?xml version="1.0" encoding="utf-8"?>
<odoo>
        <record id="res_company_form_l10n_dz_info" model="ir.ui.view">
            <field name="name">res_company_form_l10n_dz_info</field>
            <field name="model">res.company</field>
            
            <field name="inherit_id" ref="base.view_company_form"/>
            <field name="arch" type="xml">
	    		
 				 <xpath expr="//field[@name='company_registry']" position="after">
 				 	
					<field name="rc"/>
					<field name="nif"/>
					<field name="nis"/>
					<field name="ai"/>
					
				 </xpath>
				 
	       		
            </field>
        </record>

		<record id="res_partner_form_l10n_dz_info" model="ir.ui.view">
		    <field name="name">res_partner_form_l10n_dz_info</field>
		    <field name="model">res.partner</field>
			
		    <field name="inherit_id" ref="base.view_partner_form"/>
		    <field name="arch" type="xml">
			 	<xpath expr="//field[@name='property_account_position_id']" position="after">
			 		
						<field name="rc"/>
						<field name="nif"/>
						 <field name="nis"/>
						 <field name="ai"/>
					 

	       		    
	       		</xpath>
		    </field>
		</record>
</odoo>
